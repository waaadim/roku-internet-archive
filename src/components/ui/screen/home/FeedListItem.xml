<?xml version="1.0" encoding="UTF-8"?>
<component name="FeedListItem" extends="Group">
  <interface>
    <field id="itemContent" type="node" />
    <field id="itemHasFocus" type="boolean" />
    <field id="focusPercent" type="float" />
  </interface>

  <children>
    <Rectangle id="focusBorder" color="0x398ae8" opacity="0" width="312" height="308" />
    <Group id="container" translation="[6, 6]">
      <Poster id="thumbnail" width="300" height="220" loadDisplayMode="scaleToFill" />
      <Rectangle id="footer" color="0xb4c3cf" translation="[0, 150]" width="300" height="80">
        <Label id="title" color="0x242629" horizAlign="left" translation="[10, 10]" width="290"
          numLines="1" lineSpacing="5" wrap="true" />
        <Label id="releaseDate" color="0x727b83" horizAlign="left" translation="[10, 40]"
          width="196" />
      </Rectangle>
    </Group>
  </children>
</component>